#include <iostream>
#include <vector> 
using namespace std;

int main()
{
    int n=14, j = 0, a = 0;
    cin >> n;
    vector<int> v(20);
    while (n !=0){
        v[j] = n % 2;
        n /= 2;
        j++;
    }
    for (int i = 0; i <= j-1; i++){
        if (v[i] == 1) a += 1;
    }
    cout << a;
    return 0;
}
