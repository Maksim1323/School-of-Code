#include <iostream>
using namespace std;

int main()
{
    int n, n1 = 0, n2 = 0, n3 = 0;
    cin >> n;
    while (n > 34) {
        n -= 60;
        n1++;
    }
    while (n > 8) {
        n -= 10;
        n2++;
    }
    while (n > 0) {
        n -= 1;
        n3++;
    }
    cout << n3 << " " << n2 << " " << n1 << " ";
    return 0;
}
