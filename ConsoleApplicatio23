#include <iostream>
#include <vector>
using namespace std;

int main()
{
    int a = 123, b, i = 0;
    //cin >> a;
    b = a;
    vector<int> v;
    while(b > 0){
        v.push_back(b % 10); 
        b /= 10;
        ++i;
    }
    for (int i = v.size() - 1; i >= 0; --i) {
        cout << v[i] << " ";
    }
    return 0;
}
