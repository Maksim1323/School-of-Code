#include <iostream>
using namespace std;
     
int main()
    {
    int t;
    cin >> t;
    for(int i = 0; i < t; ++i){
        int h, m;
        cin >> h >> m;
        if(m > 0){
            h++;
            cout << (24 - h) * 60 + 60 - m << endl;
        }
        else{
            cout << (24 - h) * 60 << endl;
        }
    }
    return 0;
}
