//A. Не смешите мои подковы
#include<iostream>
using namespace std;

int main()
{
    int s, s2, s3, s4, i = 0;
    cin >> s >> s2 >> s3 >> s4;
    if (s == s2) ++i;
    else if (s == s3) ++i;
    else if (s == s4) ++i;
    if (s2 == s3) ++i;
    else if (s2 == s4) ++i;
    if (s3 == s4) ++i;
    cout << i;
    return 0;
}
