//Домашка на строки: №6
#include <iostream>
#include <string>
using namespace std;

int main()
{
    string s;
    cin >> s;
    int x = s.size();
    for(int i = 0; i <=x; i++){
        if (s[i] % 2 != 0) s[i] = '0';
    }
    for(int i = 0; i <= x; i++){
        if (s[i] == '0'){
            s[i] = ' ';
        }
    }
    cout << s;
    return 0;
}
