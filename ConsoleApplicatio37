//Задача №303. Коровы
#include <iostream>
using namespace std;

int main()
{
    int n, temp;
    cin >> n;
    temp = n % 10;
    if (n >= 11 && n <= 14) cout << n << " korov";
    else if (temp == 0 || (temp >= 5 && temp <= 9)) cout << n << " korov";
    else if (temp == 1) cout << n << " korova";
    else if (temp >= 2 && temp <= 4) cout << n << " korovy";
    return 0;
}
