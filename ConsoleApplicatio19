#include <iostream>
using namespace std;

int main()
{
    int a, b;
    cin >> a >> b;
    cout << "  ";
    for (int i = 1; i <= a; i++) cout << i << " ";
    cout << endl;
    for (int i = 1; i <= b; i++) {
        cout << i << " ";
        for (int j = 1; j <= a; j++) {
            cout << j * i << " ";
        }
        cout << endl;
    }
    return 0;
}
